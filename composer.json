{
    "name": "inserve/routit-api-php",
    "description": "A PHP wrapper for the RoutIT API",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.7",
        "symfony/serializer": "^6.3|^7",
        "symfony/property-access": "^6.3|^7",
        "psr/log": "^3.0",
        "phpdocumentor/reflection-docblock": "^5.3",
      "ext-dom": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11",
        "squizlabs/php_codesniffer": "^3.7",
        "phpstan/phpstan": "^2.1",
        "vimeo/psalm": "^6"
    },
    "autoload": {
        "psr-4": {
            "Inserve\\RoutITAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Inserve\\RoutITAPI\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Inserve",
            "email": "dev@inserve.nl"
        }
    ],
    "minimum-stability": "stable"
}
